#define _POSIX_C_SOURCE 200809L
#include <stdbool.h>
#include <stdio.h>
#include <stdlib.h>
#include <sys/types.h>
#include <unistd.h>

int main() {
  char *line = NULL;
  size_t len = 0;

  printf("Enter program to run. \n");
  getline(&line, &len, stdin);

  pid_t pid = fork();
  if (pid == 0) {
    execl(line, line, (char *)NULL);
    perror("execl");
  } else {
    perror("fork");
  }
}
